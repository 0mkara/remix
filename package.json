    "web3": "^0.15.3"
  },
  "devDependencies": {
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "nightwatch": "^0.9.4",
    "standard": "^7.0.1",
    "standard-reporter": "^1.0.5",
    "tape": "^4.6.0"
  },
  "scripts": {
    "start_node": "eth --rpccorsdomain \"*\" -j -v 0",
